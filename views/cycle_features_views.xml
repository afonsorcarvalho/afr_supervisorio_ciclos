<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View afr.cycle.selection View Tree -->
    <record id="view_afr_cycle_features_tree" model="ir.ui.view">
        <field name="name">view.afr.cycle.features.tree</field>
        <field name="model">afr.cycle.features</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="cycle_type_id"/>
                <field name="description"/>
                <field name="active"/>
                <field name="tempo_estimado"/>
                <field name="temperatura"/>
                <field name="pressao"/>
                
            </tree>
        </field>
    </record>

    <!-- View afr.cycle.selection form -->
    <record id="view_afr_cycle_features_form" model="ir.ui.view">
        <field name="name">view.afr.cycle.features.form</field>
        <field name="model">afr.cycle.features</field>
        <field name="arch" type="xml">
            <form string="Afr Cycle Features">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                        <group>
                            <field name="cycle_type_id"/>
                            <field name="description"/>
                            <field name="active"/>
                            <field name="tempo_estimado"/>
                            <field name="temperatura"/>
                            <field name="pressao"/>
                        </group>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- View afr.cycle.selection search -->
    <record id="view_afr_cycle_features_search" model="ir.ui.view">
        <field name="name">view.afr.cycle.features.search</field>
        <field name="model">afr.cycle.features</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action afr.cycle.features -->
    <record id="action_afr_cycle_features" model="ir.actions.act_window">
        <field name="name">Afr Cycle Features</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">afr.cycle.features</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Afr Cycle Features.
            </p>
        </field>
    </record>

</odoo>
